alias ls='ls -GFh'
alias be='bundle exec'
alias vim_install_plugins='vim +PluginInstall +qall'

alias ..='cd ..'

alias g=git
alias ga="git add"
alias gc="git commit"
alias gd="git diff"
alias gh="git help"
alias gs="git s"

alias dotfiles='cd $DOTFILES'

alias elasticsearch='lunchy start elasticsearch'

